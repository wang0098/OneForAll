{
    "name": "reCAPTCHA",
    "website": "https://www.google.com/recaptcha/",
    "matches": [
        {
            "regexp": "<div[^>]+id=\"recaptcha_image"
        },
        {
            "regexp": "<link[^>]+recaptcha"
        },
        {
            "regexp": "<div[^>]+class=\"g-recaptcha\""
        },
        {
            "search": "script",
            "regexp": "api-secure\\.recaptcha\\.net"
        },
        {
            "search": "script",
            "regexp": "recaptcha_ajax\\.js"
        },
        {
            "search": "script",
            "regexp": "/recaptcha/api\\.js"
        }
    ]
}
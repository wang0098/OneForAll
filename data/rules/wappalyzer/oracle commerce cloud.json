{
    "name": "Oracle Commerce Cloud",
    "website": "http://cloud.oracle.com/commerce-cloud",
    "matches": [
        {
            "search": "headers[oraclecommercecloud-version]",
            "regexp": "^(.+)$",
            "offset": 1
        },
        {
            "regexp": "<[^>]+id=\"oracle-cc\""
        }
    ]
}
{
    "name": "Adnegah",
    "website": "https://Adnegah.net",
    "matches": [
        {
            "search": "headers[x-advertising-by]",
            "regexp": "adnegah\\.net"
        },
        {
            "regexp": "<iframe [^>]*src=\"[^\"]+adnegah\\.net"
        },
        {
            "search": "script",
            "regexp": "[^a-z]adnegah.*\\.js$"
        }
    ]
}
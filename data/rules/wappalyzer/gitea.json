{
    "name": "Gitea",
    "website": "https://gitea.io",
    "matches": [
        {
            "search": "cookies[i_like_gitea]",
            "regexp": ".*"
        },
        {
            "regexp": "<div class=\"ui left\">\\n\\s+\u00a9 Gitea Version: ([\\d.]+)",
            "offset": 1
        },
        {
            "search": "meta[keywords]",
            "regexp": "^go,git,self-hosted,gitea$"
        }
    ]
}